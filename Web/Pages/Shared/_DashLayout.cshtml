            <div class=" text-center text-gray-900 text-lg font-semibold  leading-normal">
                @Localizer["Create"]
            </div>

            <div class="flex flex-col">



                <a href="/dtcms/article/upsert" class="text-gray-700 text-sm font-semibold leading-normal flex items-center border-b border-solid border-gray-100 pb-2 mb-6">
                    <span class="icon-Book-open text-xl rtl:ml-2 ltr:mr-2"></span>
                    @Localizer["AddFastArticle"]
                </a>

                <a href="/dtcms/podcast/upsert" class="text-gray-700 text-sm font-semibold leading-normal flex items-center border-b border-solid border-gray-100 pb-2 mb-6">
                    <span class="icon-Microphone text-xl rtl:ml-2 ltr:mr-2"></span>
                  پادکست
                </a>
                <a href="/dtcms/portfolio/upsert" class="text-gray-700 text-sm font-semibold leading-normal flex items-center border-b border-solid border-gray-100 pb-2 mb-6">
                    <span class="icon-Desktop-computer text-xl rtl:ml-2 ltr:mr-2"></span>
                                                                            نمونه کار
                </a>

