        <div id="languageDrawer"
             class="fixed bottom-0 translate-y-full pb-12 px-4 pt-2  w-96 md:w-[510px] rounded-t-xl z-50 mx-auto  bg-white"
             tabindex="-1" aria-labelledby="drawer-bottom-label ">
            <div class="  text-center">
                <div class="mb-5 h-[5px] w-[50px] bg-gray-300 rounded-md mx-auto"></div>
                <span class="font-semibold text-base text-gray-900 inline-block mb-4">@Localizer["LanguageControlTitle"]</span>
                <div class="space-y-4 mb-9">
                    <div class="flex flex-col space-y-2 items-start ltr:text-left ltr:direction__ltr">
                        <label for="">@Localizer["StieLanguage"]</label>
                        <select id=siteLangSelector
                                class="w-full appearance-none rounded-md border border-gray-300 bg-white px-3 py-2.5 text-right ltr:text-left ltr:bg-right-center  placeholder-gray-400 shadow-sm focus:border-indigo-500 focus:outline-none focus:ring-indigo-500 sm:text-sm ltr:select_ltr"
                                data-lang="@Lang">
                            <option value="" disabled>@Localizer["StieLanguageOption1"]</option>
                            <option value="fa-IR">@Localizer["StieLanguageOption2"]</option>
                            <option value="en-US">@Localizer["StieLanguageOption3"]</option>
                            <option value="ar-SA">@Localizer["StieLanguageOption4"]</option>
                        </select>
                    </div>
                    <div class="flex flex-col space-y-2 items-start ltr:text-left ltr:direction__ltr">
                        <label for="">@Localizer["SystemLanguage"]</label>
                        <select id="systemLangSelector"
                                data-lang="@Lang"
                                class="w-full appearance-none rounded-md border border-gray-300 bg-white px-3 py-2.5 text-right ltr:text-left ltr:bg-right-center  placeholder-gray-400 shadow-sm focus:border-indigo-500 focus:outline-none focus:ring-indigo-500 sm:text-sm ltr:select_ltr">
                            <option value="" disabled>@Localizer["SystemLanguageOption1"]</option>
                            <option value="fa-IR">@Localizer["SystemLanguageOption2"]</option>
                            <option value="en-US">@Localizer["SystemLanguageOption3"]</option>
                        </select>
                    </div>
                </div>
                <div class="flex justify-between items-center flex-row ">
                    <button onclick="changeLanguage()"
                            class="font-normal text-sm text-white bg-green-400 py-[9px] px-[18px] rounded-md">
                        @Localizer["SaveButton"]
                    </button>
                    <button onclick="hideLanguageDrawer()"
                            class="font-normal text-sm text-gray-600 py-[9px] px-[18px] rounded-md">
                        @Localizer["CancelButton"]
                    </button>
                </div>
            </div>
        </div>

        <div id="logoutDrawer"
             class="fixed bottom-0 translate-y-full  w-96 md:w-[510px] rounded-t-xl px-4 py-3  z-40 p-4 mx-auto  bg-white"
             tabindex="-1" aria-labelledby="drawer-bottom-label">
            <div class="flex justify-center items-center flex-col py-3">
                <svg class="mb-4" width="50" height="5" viewBox="0 0 50 5" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <rect x="0.5" width="49" height="5" rx="2.5" fill="#D1D5DB" />
                </svg>
                <h3 class="text-base font-semibold">آیا خروج از حساب کاربری را تایید میکنید؟</h3>
                <div class="flex justify-between mt-9 mb-4 w-full px-5">
                    <button onclick="hideLogoutDrawer()">لغو</button>
                    <button class="bg-red-600 text-white rounded-md text-sm px-5 py-3">خروج از سامانه</button>
                </div>
            </div>
        </div>
