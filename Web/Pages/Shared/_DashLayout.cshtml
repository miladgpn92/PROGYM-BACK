                                <div class="w-[0.5px] h-7 bg-white mx-2"></div>

                                <button onclick="document.getElementById('frmLogout').submit()" type="submit" class="item group mx-1 flex items-center px-3 py-1 rounded-md hover:bg-red-600 " target="_blank">
                                    <svg xmlns="http://www.w3.org/2000/svg" class="group-hover:fill-white" height="24px" viewBox="0 -960 960 960" width="24px" fill="#ffffff"><path d="M200-120q-33 0-56.5-23.5T120-200v-560q0-33 23.5-56.5T200-840h280v80H200v560h280v80H200Zm440-160-55-58 102-102H360v-80h327L585-622l55-58 200 200-200 200Z"/>
                                    </svg>

                                    <span class="text-sm font-normal text-white mr-2 group-hover:text-white">خروج</span>
                                </button>
                               

                                <button class="item block xd:hidden" id="backButton">
                                  <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M10 19L3 12M3 12L10 5M3 12L21 12" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                   </svg>

                                </button>
                                <form asp-action="Logout" asp-controller="Account" method="post" id="frmLogout"></form>
                            </div>
                        </div>
                    </div>
                </header>
                <div class="container px-4 max-w-5xl mx-auto pb-20">
                    @RenderBody()
                </div>
            </div>
        </div>
